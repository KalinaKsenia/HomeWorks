abstract class Metal {
    abstract int getEndurance();
    }
class Steel extends Metal {
    @Override
    int getEndurance() {
        return 50;
    }
}
class Copper extends Metal {
    @Override
    int getEndurance() {
        return 20;
    }
}
class Iron extends Metal {
    @Override
    int getEndurance() {
        return 30;
    }
}
