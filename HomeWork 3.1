     class Sword<M extends Metal> {
         private M material;

         public Sword(M material) {
             this.material = material;
         }
         public boolean checkEndurance() {
             return material.getEndurance() > 49;
         }
     }
     public class Test{
         public static void main(String[] args) {
             Sword<Steel> steelSword = new Sword<>(new Steel());
             System.out.println("Меч из стали прошел проверку прочности: " + steelSword.checkEndurance());
         }
     }

